int add(int n)
{
	int sum, x;
	while (n!= 0)
	{
		scanf(x);
		sum = sum + x;
		n = n - 1;
	}
	return (sum);
}
int subt(int n)
{
	int sum, x;
	while (n!= 0)
	{
		scanf(x);
		sum = sum - x;
		n = n - 1;
	}
	return (sum);
}
void main()
{
	int a, b;
	a = 5;
	b = a;
	if(a == b)
	{
		add(a);
		subt(b);
	}
	else
	{
		printf("error", b);
	}
}